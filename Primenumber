
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,a=0,i;
    cin>>n;
    for(i=2;i<=int(sqrt(n));i++)
    {
       if(n%i==0)
       {
         a+=1;
       }
    cout<<a<<endl;
    if(a==0)
    {
       cout<<"prime number";
     }
    else 
    {
       cout<<"not prime number";
    }
}
